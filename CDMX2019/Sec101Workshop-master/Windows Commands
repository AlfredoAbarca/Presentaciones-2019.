8a. Expand-Archive -path .\winlogbeat.zip -destinationpath .\
8b. cd .\winlogbeat-7.2.0-windows-x86_64\
8c. .\install-service-winlogbeat.ps1

12a.	.\winlogbeat.exe setup â€“dashboards

13a. Start-Service Winlogbeat

